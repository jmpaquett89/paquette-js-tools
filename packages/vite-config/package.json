{
  "name": "@paquette/vite-config",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./base": {
      "import": "./dist/vite.base.config.mjs",
      "require": "./dist/vite.base.config.cjs"
    },
    "./dev": {
      "import": "./dist/vite.dev.config.mjs",
      "require": "./dist/vite.dev.config.cjs"
    },
    "./prod": {
      "import": "./dist/vite.prod.config.mjs",
      "require": "./dist/vite.prod.config.cjs"
    }
  },
  "files": [
    "./**.*.config.ts"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-externalize-deps": "^0.8.0"
  }
}
